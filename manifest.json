{
  "vendor": "zaelab",
  "name": "visit-counter",
  "version": "0.0.1",
  "title": "Visit counter",
  "description": "Visit counter component",
  "defaultLocale": "en-US",
  "builders": {
    "messages": "1.x",
    "store": "0.x",
    "react": "3.x",
    "node": "6.x",
    "graphql": "1.x"
  },
  "policies": [
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/dataentities/*"
      }
    }
  ],
  "dependencies": {
    "vtex.styleguide": "9.x",
    "vtex.css-handles": "0.x",
    "vtex.search-graphql": "0.x"
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
